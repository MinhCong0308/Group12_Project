package screen.sortingApp.color;
import java.awt.Color;
public class ColorManager {
	//Arguments a,b,c in Color(a,b,c) is RGB code of the suitable color
	public static Color BACKGROUND = new Color(0,255,255);
	public static Color BAR_WHITE = new Color(211,222,228);
	public static Color BAR_BLUE = new Color(121,158,196);
	public static Color BAR_CYAN = new Color(0,254,252);
	public static Color BAR_RED = new Color(221,77,81);
	
	
}
